<odoo>
    <record id="ir_cron_zatca_check" model="ir.cron">
        <field name="name">ZATCA: Check invoice status</field>
        <field name="model_id" ref="account.model_account_move"/>
        <field name="state">code</field>
        <field name="code">model.search([('zatca_status','in',['sent'])]).action_check_zatca_status()</field>
        <field name="interval_number">10</field>
        <field name="interval_type">minutes</field>
        <field name="numbercall">-1</field>
    </record>
</odoo>